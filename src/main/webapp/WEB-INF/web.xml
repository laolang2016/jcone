<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>CharsetEncodingFilter</filter-name>
  	<display-name>CharsetEncodingFilter</display-name>
  	<description></description>
  	<filter-class>com.laolang.filter.CharsetEncodingFilter</filter-class>
  	<init-param>
  		<param-name>code</param-name>
  		<param-value>UTF-8</param-value>
  	</init-param>
  </filter>
  <filter-mapping>
  	<filter-name>CharsetEncodingFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>CriticismServlet</servlet-name>
  	<display-name>CriticismServlet</display-name>
  	<description></description>
  	<servlet-class>com.laolang.servlet.CriticismServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MessageContentServlet</servlet-name>
  	<display-name>MessageContentServlet</display-name>
  	<description></description>
  	<servlet-class>com.laolang.servlet.MessageContentServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<display-name>loginServlet</display-name>
  	<description></description>
  	<servlet-class>com.laolang.servlet.loginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MessageListServlet</servlet-name>
  	<display-name>MessageListServlet</display-name>
  	<description></description>
  	<servlet-class>com.laolang.servlet.MessageListServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PublishNewMessageServlet</servlet-name>
  	<display-name>PublishNewMessageServlet</display-name>
  	<description></description>
  	<servlet-class>com.laolang.servlet.PublishNewMessageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReplyServlet</servlet-name>
  	<display-name>ReplyServlet</display-name>
  	<description></description>
  	<servlet-class>com.laolang.servlet.ReplyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CriticismServlet</servlet-name>
  	<url-pattern>/CriticismServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MessageContentServlet</servlet-name>
  	<url-pattern>/MessageContentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MessageListServlet</servlet-name>
  	<url-pattern>/MessageListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PublishNewMessageServlet</servlet-name>
  	<url-pattern>/PublishNewMessageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReplyServlet</servlet-name>
  	<url-pattern>/ReplyServlet</url-pattern>
  </servlet-mapping>
</web-app>
